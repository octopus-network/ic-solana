type TokenInfo = record {
  uri : text;
  decimals : nat8;
  name : text;
  symbol : text;
};
service : (text, text) -> {
  create_associated_token_account : (text, text, text) -> (text);
  create_ata : (text, text) -> (text);
  create_token : () -> (text);
  create_token_with_metadata : (TokenInfo) -> (text);
  get_payer : () -> (text);
  mint_to : (text, nat64, text) -> (text);
  query_transaction : (text, text) -> (text);
  update_token_metadata : (text, TokenInfo) -> (text);
}
